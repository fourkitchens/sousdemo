<div class="cl-icons-example" id="icons">
  {% for item in items %}
    <div class="cl-icons-example__item">
      {% include "@atoms/images/icons/_icon.twig" with {
        icon__name: item.value,
        icon__role: item.role,
        icon__title: item.title,
        icon__desc: item.desc,
        icon__decorative: item.decorative,
      } %}
      <pre>{{ item.value }}</pre>
    </div>
  {% endfor %}
</div>
<br>